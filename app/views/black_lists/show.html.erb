<div class="panel panel-default">
  <div class="panel-heading">
    <h2 class="panel-title">Lista Negra</h2>
  </div>

	<p>
	  <strong>Name:</strong>
	  <%= @black_list.name %>
	</p>

	<p>
	  <strong>Fone:</strong>
	  <%= @black_list.fone %>
	</p>

	<p>
	  <strong>Cpf:</strong>
	  <%= @black_list.cpf %>
